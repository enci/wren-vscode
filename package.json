{
  "name": "wren",
  "displayName": "Wren Language",
  "description": "Visual Studio Code extension for the Wren programming language",
  "version": "0.1.1",
  "publisher": "BojanEndrovski",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "wren",
    "programming language"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enci/wren-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/enci/wren-vscode/issues"
  },
  "homepage": "https://github.com/enci/wren-vscode#readme",
  "license": "MIT",
  "icon": "resources/icon.png",
  "main": "./out/extension.js",
  "activationEvents": [],
  "contributes": {
    "languages": [
      {
        "id": "wren",
        "extensions": [".wren"],
        "aliases": ["Wren"],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./resources/icons/wren-file-light.svg",
          "dark": "./resources/icons/wren-file-dark.svg"
        }
      }
    ],
    "grammars": [
      {
          "language": "wren",
          "scopeName": "source.wren",
          "path": "./syntaxes/wren.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@types/vscode": "^1.105.0",
    "@vscode/vsce": "^3.6.2",
    "typescript": "^5.9.3"
  }
}
